#include <stdio.h>
#include <stdlib.h>

int removeDuplicates(int* arr, int n, int* unique) {
    int j = 0;
    unique[j++] = arr[0];
    for (int i = 1; i < n; i++) {
        if (arr[i] != arr[i - 1]) {
            unique[j++] = arr[i];
        }
    }
    return j;
}

void climbingLeaderboard(int* ranked, int n, int* player, int m) {
    int unique[n];
    int uniqueCount = removeDuplicates(ranked, n, unique);

    int result[m];
    int i = uniqueCount - 1;

    for (int j = 0; j < m; j++) {
        int score = player[j];

        while (i >= 0 && score >= unique[i]) {
            i--;
        }

        result[j] = i + 2;
    }

    for (int j = 0; j < m; j++) {
        printf("%d\n", result[j]);
    }
}

int main() {
    int n;
    scanf("%d", &n);

    int ranked[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &ranked[i]);
    }

    int m;
    scanf("%d", &m);

    int player[m];
    for (int i = 0; i < m; i++) {
        scanf("%d", &player[i]);
    }

    climbingLeaderboard(ranked, n, player, m);

    return 0;
}
