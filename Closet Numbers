#include <stdio.h>
#include <stdlib.h>
#include <limits.h>

int compare(const void *a, const void *b) {
    return (*(int *)a - *(int *)b);
}

void closestNumbers(int arr[], int n) {
    qsort(arr, n, sizeof(int), compare);

    int minDiff = INT_MAX;
    for (int i = 1; i < n; i++) {
        int diff = abs(arr[i] - arr[i - 1]);
        if (diff < minDiff) {
            minDiff = diff;
        }
    }

    for (int i = 1; i < n; i++) {
        if (abs(arr[i] - arr[i - 1]) == minDiff) {
            printf("%d %d ", arr[i - 1], arr[i]);
        }
    }
    printf("\n");
}

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    closestNumbers(arr, n);

    return 0;
}
